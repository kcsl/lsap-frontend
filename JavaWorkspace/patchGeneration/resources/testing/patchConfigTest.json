{
	"MutexFunctionCriteria": {
		"ignore_all_locks" : false,
		"mutex" : true,
		"lock" : true
	},
	
	"MutexMacroCriteria": {
		"ignore_all_locks" : false,
		"mutex" : true
	},
	
	"MutexFunctionsToInclude": [
		"int function(void *lock);"
	],
	
	"MutexMacrosToInclude": [
		"#define macro(lock) function(lock)"
	],
	
	"SpinFunctionCriteria": {
		"ignore_all_locks" : true
	},
	
	"SpinMacroCriteria": {
		"ignore_all_locks" : false,
		"lock" : true
	},
	
	"SpinFunctionsToInclude": [
		"void __raw_spin_lock(void *lock);",
		"int __raw_spin_trylock(void *lock);",
	],
	
	"SpinMacrosToInclude": [
		"#define macro(lock) __raw_spin_lock(lock)"
	],
	
	"MutexPathsToRead": [
		"include/linux/mutex.h"
	],
	
	"MutexPathsToChange": [
		"include/linux/mutex.h"
	],
	
	"SpinPathsToRead": [
		"include/linux/spinlock.h"
	],
	
	"SpinPathsToChange": [
		"include/linux/spinlock.h"
	],
	
	"FunctionReturnTypes" : [
		"int",
		"void",
		"struct patchTest *"
	]
}